<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <FrameLayout
        android:id="@+id/frameLayout"
        android:layout_width="match_parent"
        android:layout_height="102dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_user_purple" />

    </FrameLayout>

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="13dp"
        android:text="Car driving license"
        android:textColor="#000000"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frameLayout" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        android:text="Know how to ride a bike"
        android:textColor="#000000"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:text="Know how to ride an electric scooter"
        android:textColor="#000000"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="59dp"
        android:text="Maximum walking distance"
        android:textColor="#000000"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/textView9"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="56dp"
        android:text="Public transport pass"
        android:textColor="#000000"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/textView11"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="122dp"
        android:layout_height="25dp"
        android:layout_marginTop="96dp"
        android:text="Possess a vehicle"
        android:textColor="#000000"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/textView12"
        app:layout_constraintTop_toBottomOf="@+id/textView12" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        android:text="Expiry date:"
        android:textColor="#000000"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/busYesID"
        app:layout_constraintTop_toBottomOf="@+id/textView12" />

    <Button
        android:id="@+id/cancelButtonID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:layout_marginBottom="16dp"
        android:inputType="textCapSentences"
        android:text="Cancel"
        app:cornerRadius="25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/saveButtonID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="78dp"
        android:inputType="textCapSentences"
        android:text="Save"
        app:cornerRadius="25dp"
        app:layout_constraintBottom_toBottomOf="@+id/cancelButtonID"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/cancelButtonID" />

    <View
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:background="#6750A3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frameLayout" />

    <CheckBox
        android:id="@+id/carYesID"
        android:layout_width="67dp"
        android:layout_height="23dp"
        android:layout_marginTop="12dp"
        android:text="@string/yes"
        app:layout_constraintStart_toStartOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <CheckBox
        android:id="@+id/carNoID"
        android:layout_width="77dp"
        android:layout_height="29dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="12dp"
        android:text="@string/no"
        app:layout_constraintStart_toEndOf="@+id/carYesID"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <CheckBox
        android:id="@+id/bikeYesID"
        android:layout_width="73dp"
        android:layout_height="29dp"
        android:text="@string/yes"
        app:layout_constraintStart_toStartOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <CheckBox
        android:id="@+id/bikeNoID"
        android:layout_width="68dp"
        android:layout_height="31dp"
        android:layout_marginStart="2dp"
        android:text="@string/no"
        app:layout_constraintStart_toStartOf="@+id/carNoID"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <CheckBox
        android:id="@+id/scooterYesID"
        android:layout_width="74dp"
        android:layout_height="29dp"
        android:layout_marginTop="4dp"
        android:text="@string/yes"
        app:layout_constraintStart_toStartOf="@+id/textView9"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <CheckBox
        android:id="@+id/scooterNoID"
        android:layout_width="75dp"
        android:layout_height="34dp"
        android:text="@string/no"
        app:layout_constraintStart_toStartOf="@+id/bikeNoID"
        app:layout_constraintTop_toBottomOf="@+id/textView9"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/maximumDistEditTextID"
        android:layout_width="195dp"
        android:layout_height="34dp"
        android:layout_marginTop="10dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Maximum distance"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="@+id/textView11"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="12dp"
        android:text="meters"
        android:textColor="#000000"
        app:layout_constraintStart_toEndOf="@+id/maximumDistEditTextID"
        app:layout_constraintTop_toTopOf="@+id/maximumDistEditTextID" />


    <EditText
        android:id="@+id/expiryDayTextID"
        android:layout_width="168dp"
        android:layout_height="34dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="12dp"
        android:inputType="none"
        android:text="Date"
        android:textColor="#000000"
        android:textSize="10sp"
        app:layout_constraintStart_toEndOf="@+id/textView14"
        app:layout_constraintTop_toBottomOf="@+id/busNoID" />

    <CheckBox
        android:id="@+id/busYesID"
        android:layout_width="68dp"
        android:layout_height="33dp"
        android:layout_marginTop="7dp"
        android:text="@string/yes"
        app:layout_constraintStart_toStartOf="@+id/textView12"
        app:layout_constraintTop_toBottomOf="@+id/textView12" />

    <CheckBox
        android:id="@+id/busNoID"
        android:layout_width="62dp"
        android:layout_height="32dp"
        android:layout_marginStart="3dp"
        android:layout_marginTop="8dp"
        android:text="@string/no"
        app:layout_constraintStart_toStartOf="@+id/scooterNoID"
        app:layout_constraintTop_toBottomOf="@+id/textView12" />

    <CheckBox
        android:id="@+id/checkBox9"
        android:layout_width="92dp"
        android:layout_height="32dp"
        android:layout_marginTop="10dp"
        android:text="Car"
        app:layout_constraintStart_toStartOf="@+id/textView13"
        app:layout_constraintTop_toBottomOf="@+id/textView13" />

    <CheckBox
        android:id="@+id/checkBox10"
        android:layout_width="95dp"
        android:layout_height="29dp"
        android:text="Bike"
        app:layout_constraintStart_toStartOf="@+id/checkBox9"
        app:layout_constraintTop_toBottomOf="@+id/checkBox9" />

    <CheckBox
        android:id="@+id/checkBox11"
        android:layout_width="92dp"
        android:layout_height="31dp"
        android:layout_marginStart="1dp"
        android:text="Scooter"
        app:layout_constraintStart_toStartOf="@+id/checkBox10"
        app:layout_constraintTop_toBottomOf="@+id/checkBox10" />

    <Button
        android:id="@+id/selectDateButtonID"
        android:layout_width="47dp"
        android:layout_height="44dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="9dp"
        android:drawableTop="@drawable/ic_baseline_calendar_month_24"
        android:textSize="10sp"
        app:layout_constraintStart_toEndOf="@+id/expiryDayTextID"
        app:layout_constraintTop_toBottomOf="@+id/busNoID" />

</androidx.constraintlayout.widget.ConstraintLayout>