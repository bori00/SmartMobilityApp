<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RadioGroup
        android:id="@+id/radio_group_4"
        android:layout_width="127dp"
        android:layout_height="57dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toStartOf="@+id/textView12"
        app:layout_constraintTop_toBottomOf="@+id/textView12">

        <RadioButton
            android:id="@+id/busYesID"
            android:layout_width="80dp"
            android:layout_height="20dp"
            android:text="Yes" />

        <RadioButton
            android:id="@+id/busNoID"
            android:layout_width="71dp"
            android:layout_height="20dp"
            android:layout_marginTop="5dp"
            android:text="No" />

    </RadioGroup>

    <RadioGroup
        android:id="@+id/radio_group_1"
        android:layout_width="127dp"
        android:layout_height="57dp"
        android:layout_marginTop="5dp"
        app:layout_constraintStart_toStartOf="@+id/textView9"
        app:layout_constraintTop_toBottomOf="@+id/textView9">

        <RadioButton
            android:id="@+id/scooterYesID"
            android:layout_width="80dp"
            android:layout_height="20dp"
            android:text="Yes" />

        <RadioButton
            android:id="@+id/scooterNoID"
            android:layout_width="71dp"
            android:layout_height="20dp"
            android:layout_marginTop="5dp"
            android:text="No" />

    </RadioGroup>

    <RadioGroup
        android:id="@+id/radio_group_2"
        android:layout_width="127dp"
        android:layout_height="57dp"
        android:layout_marginTop="5dp"
        app:layout_constraintStart_toStartOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/textView8">

        <RadioButton
            android:id="@+id/bikeYesID"
            android:layout_width="80dp"
            android:layout_height="20dp"
            android:text="Yes" />

        <RadioButton
            android:id="@+id/bikeNoID"
            android:layout_width="71dp"
            android:layout_height="20dp"
            android:layout_marginTop="5dp"
            android:text="No" />

    </RadioGroup>

    <FrameLayout
        android:id="@+id/frameLayout"
        android:layout_width="406dp"
        android:layout_height="73dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_user_purple" />

    </FrameLayout>

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="4dp"
        android:text="Car driving license"
        android:textColor="#000000"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frameLayout" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="76dp"
        android:text="Know how to ride a bike"
        android:textColor="#000000"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="72dp"
        android:text="Know how to ride an electric scooter"
        android:textColor="#000000"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/textView8"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="68dp"
        android:text="Maximum walking distance"
        android:textColor="#000000"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/textView9"
        app:layout_constraintTop_toBottomOf="@+id/textView9" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="56dp"
        android:text="Public transport pass"
        android:textColor="#000000"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/textView11"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="122dp"
        android:layout_height="25dp"
        android:layout_marginTop="132dp"
        android:text="Possess a vehicle"
        android:textColor="#000000"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/textView12"
        app:layout_constraintTop_toBottomOf="@+id/textView12" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Expiry date:"
        android:textColor="#000000"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/expiryDayTextID"
        app:layout_constraintStart_toStartOf="@+id/textView12"
        app:layout_constraintTop_toTopOf="@+id/expiryDayTextID"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/cancelButtonID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:layout_marginBottom="16dp"
        android:inputType="textCapSentences"
        android:text="Cancel"
        app:cornerRadius="25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/saveButtonID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="78dp"
        android:inputType="textCapSentences"
        android:text="Save"
        app:cornerRadius="25dp"
        app:layout_constraintBottom_toBottomOf="@+id/cancelButtonID"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/cancelButtonID" />


    <EditText
        android:id="@+id/maximumDistEditTextID"
        android:layout_width="195dp"
        android:layout_height="34dp"
        android:layout_marginTop="10dp"
        android:ems="10"
        android:hint="@string/maximum_walking_distance"
        android:inputType="textPersonName"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="@+id/textView11"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="12dp"
        android:text="meters"
        android:textColor="#000000"
        app:layout_constraintStart_toEndOf="@+id/maximumDistEditTextID"
        app:layout_constraintTop_toTopOf="@+id/maximumDistEditTextID" />


    <EditText
        android:id="@+id/expiryDayTextID"
        android:layout_width="168dp"
        android:layout_height="34dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="80dp"
        android:inputType="none"
        android:textColor="#000000"
        android:textSize="10sp"
        app:layout_constraintStart_toEndOf="@+id/textView14"
        app:layout_constraintTop_toBottomOf="@+id/textView12" />

    <CheckBox
        android:id="@+id/carCheckID"
        android:layout_width="92dp"
        android:layout_height="32dp"
        android:layout_marginTop="10dp"
        android:text="Car"
        app:layout_constraintStart_toStartOf="@+id/textView13"
        app:layout_constraintTop_toBottomOf="@+id/textView13" />

    <CheckBox
        android:id="@+id/bikeCheckID"
        android:layout_width="95dp"
        android:layout_height="29dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="12dp"
        android:text="Bike"
        app:layout_constraintStart_toEndOf="@+id/carCheckID"
        app:layout_constraintTop_toBottomOf="@+id/textView13" />

    <CheckBox
        android:id="@+id/scooterCheckID"
        android:layout_width="92dp"
        android:layout_height="31dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="12dp"
        android:text="Scooter"
        app:layout_constraintStart_toEndOf="@+id/bikeCheckID"
        app:layout_constraintTop_toBottomOf="@+id/textView13" />

    <Button
        android:id="@+id/selectDateButtonID"
        android:layout_width="47dp"
        android:layout_height="44dp"
        android:layout_marginStart="16dp"
        android:drawableTop="@drawable/ic_baseline_calendar_month_24"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="@+id/expiryDayTextID"
        app:layout_constraintStart_toEndOf="@+id/expiryDayTextID"
        app:layout_constraintTop_toTopOf="@+id/expiryDayTextID" />

    <RadioGroup
        android:id="@+id/radio_group_3"
        android:layout_width="127dp"
        android:layout_height="57dp"
        android:layout_marginTop="12dp"
        app:layout_constraintStart_toStartOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/textView7">

        <RadioButton
            android:id="@+id/carYesID"
            android:layout_width="80dp"
            android:layout_height="20dp"
            android:text="Yes" />

        <RadioButton
            android:id="@+id/carNoID"
            android:layout_width="71dp"
            android:layout_height="20dp"
            android:layout_marginTop="5dp"
            android:text="No" />

    </RadioGroup>

    <View
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:background="#6750A3"
        tools:layout_editor_absoluteX="5dp"
        tools:layout_editor_absoluteY="73dp" />

</androidx.constraintlayout.widget.ConstraintLayout>